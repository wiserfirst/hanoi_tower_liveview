<div class="subcontainer" phx-hook="DropContainer" phx-value-rodno="<%= @val %>">
  <div class="rod">
    <div class="disk-container">
    <%= for {n, index} <- Enum.with_index(@rod) do %>
      <div class="<%= "disk disk#{n}" %>" phx-hook="Disk" draggable="<%= index === 0 %>"></div>
    <% end %>
    </div>
  </div>
</div>
